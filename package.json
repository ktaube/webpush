{
  "name": "webpush",
  "scripts": {
    "build": "cd frontend && bun run build",
    "dev": "dotenvx run -f .env.local -- bun run --filter '*' dev",
    "dev:tunnel": "dotenvx run -f .env -- bun run --filter '*' dev:tunnel"
  },
  "workspaces": [
    "backend",
    "frontend",
    "tunnel"
  ],
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0"
  }
}
